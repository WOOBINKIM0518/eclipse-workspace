package Exercise_Q5;

public class Sonata extends Car {
	@Override
	public void startCar() {
		System.out.println("Sonata 시동을켭니다.");
	}

	@Override
	public void driveCar() {
		System.out.println("Sonata 달립니다.");
	}

	@Override
	public void stopCar() {
		System.out.println("Sonata 멉춥니다.");
	}

	@Override
	public void turnOff() {
		System.out.println("Sonata 시동 끕니다.");
	}

}
