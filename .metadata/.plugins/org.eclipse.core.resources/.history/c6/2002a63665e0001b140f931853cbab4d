package gamelevel;

public class Player {

	private PlayLevel level;
	
	public player() {
	level = new BegingerLevel();
	level.showLevelMessage();
}
	public PlayerLevel() {
		return level;
	}
	
	public void upgradeLevel() {
		this.level = level;
		this.showLevelMessage();
	}
	public void pay(int count) {
		level.go(count);
	}
}
