package inheritance4;


/*public*/	class A { // 廃 class 照拭 廃 public
				public A() {
			System.out.println("持失切 A");	}}

			class B extends A{
				public B() {
				System.out.println("持失切 B");}}

			class C extends B {
				public C() {
				System.out.println("持失切 C");	}}

public class ConstructorEx02 {

	public static void main(String[] args) {
		
		C c ; 
		c = new C();

	}

}
