package gamelevel;

public class BeginnerLevel extends PlayerLevel {

	@Override
	void run() {
		System.out.println("천천히 달립니다.");
		
	}

	@Override
	void jupm() {
		System.out.println("Jump할 줄 모르지롱.");
	}

	@Override
	void tunr() {
		 System.out.println("Turn 할 줄 모르지롱.");
	}

	@Override
	void showLevelMessage() {
		System.out.println("***** 초보자 레벨입니다. *****");
	}
}
