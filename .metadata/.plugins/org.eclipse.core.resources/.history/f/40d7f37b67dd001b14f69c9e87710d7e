package woobin;

public class Test_15_3 {

	public static void main(String[] args) {
		int a,b; // a = 10의 자리 , b =1의 자리
		
		for(int i = 1 ; i<100 ; i++) {
			a= i/10;
			b= i%10; 
			
			if((a==3|| a==6 || a==9) && (b==3 ||b==6||b==9)) {  // 10자리 3,6,9 그리고 일의자리 3,6,9 
				System.out.println(i + "박수 짝짝");
			}
			else if (( a==3 || a==6 || a==9) && (b !=3||b!=6 || b!=9)) {// 10의자리 3,6,9 일의자리 노상광
				System.out.println((i + "박수 짝"));
			}
			else if(( a!=3 || a!=6 ||a!=9) && (b ==3||b==6 || b==9)) {// 1의 자리 3,6,9 10자리 노상관
				System.out.println((i + "박수 짝"));}
			
		}
	}
}


