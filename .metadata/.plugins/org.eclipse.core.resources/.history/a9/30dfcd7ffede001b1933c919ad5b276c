package polymorphism;

class AnimalTset {
	public void monve() {
		System.out.println("동물이 움직입니다.");
	}
}

class Human extends AnimalTset{
	public void move() {
		System.out.println("사람이 두 발로 걷습니다.");
	}
}

class Tiger extends AnimalTset{
	public void monve() {
		System.out.println("호랑이가 네 발로 뜁니다");
	}
}

class Eagle extends AnimalTset{
	public void monve() {
		System.out.println("독수리가 하늘을 납니다.");
	}
}

